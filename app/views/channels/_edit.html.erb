<div class="modal fade bd-example-modal-lg" id="editchannel" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
  <div class="modal-dialog modal-lg">
    <div class="modal-content">
       <h3>Edit <%= @channel.name %></h3>
         <%= simple_form_for @channel do |f| %>
          <%= f.input :name %>
          <%= f.submit %>
         <% end %>
    </div>
  </div>
</div>
